#!/usr/bin/env node
var RunAll = require('./utils/run-all');
var CommanderWrapper = require('./utils/commander-wrapper');

var program = CommanderWrapper(function(commander) {
  return commander;
});

var commands = [
  ['sb-build-all'],
  ['sb-lint-all'],
  ['sb-test-all'],
];
var args = ['--watch'];

RunAll(commands, args);
