{
  "name": "videojs-spellbook",
  "version": "4.0.1",
  "description": "An official web project builder for video.js and its plugins",
  "scripts": {
    "premanual": "npm run clean",
    "manual": "node -e 'var t = require(\"./test/unit/test-helper\");d=\"./test/fixtures/test-pkg-main\";t.prototype.gitInit(d);t.prototype.npmLink(d);'",
    "clean": "npm-run-all -p clean:*",
    "clean:manual": "node -e 'require(\"./test/unit/test-helper\").prototype.fixtureClean();'",
    "clean:dist": "rimraf ./dist",
    "pretest": "npm run build",
    "test": "mocha 'test/unit/**/*.test.js'",
    "prepublish": "npm run build",
    "prebuild": "npm run clean",
    "precommit": "npm run build:docs && git add ./TODO.md ./README.md",
    "build": "npm-run-all -p build:*",
    "build:docs": "SB_INTERNAL=1 node ./src/sb-lint-docs-md --fix ./TODO.md ./README.md",
    "build:man": "node scripts/generate-man-pages.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/videojs/spellbook"
  },
